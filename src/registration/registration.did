type Error = record { message : text };
type Result = variant { Ok : text; Err : Error };
type UserInfo = record {
  accounts : vec principal;
  first_name : text;
  last_name : text;
};
service : () -> {
  add_account : (principal, principal) -> ();
  add_subaccount : () -> (Result);
  get_user : (principal) -> (opt UserInfo) query;
  load_wallet_wasm : () -> ();
  register_user : (principal, text, text) -> ();
  user_exists : (principal) -> (bool) query;
}
